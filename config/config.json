{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "gen-grid-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/genGrid/GenGridWebPart.js",
          "manifest": "./src/webparts/genGrid/GenGridWebPart.manifest.json"
        }
      ]
    },
    "gen-form-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/genForm/GenFormWebPart.js",
          "manifest": "./src/webparts/genForm/GenFormWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": {
      "path": "https://code.jquery.com/jquery-1.12.4.min.js",
      "globalName": "jquery"
    },
    "datatables.net": "https://cdn.datatables.net/1.10.15/js/jquery.dataTables.min.js"
  },
  "localizedResources": {
    "GenGridWebPartStrings": "lib/webparts/genGrid/loc/{locale}.js",
    "GenFormWebPartStrings": "lib/webparts/genForm/loc/{locale}.js"
  }
}
